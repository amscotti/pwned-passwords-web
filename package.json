{
  "name": "pwned-passwords-web",
  "version": "0.2.0",
  "description": "Web UI for checking to see if your password has been compromised in any data breach",
  "scripts": {
    "build": "rimraf dist && webpack --mode=production --bail --progress --profile",
    "server": "webpack-dev-server --history-api-fallback --inline --progress",
    "start": "npm run server"
  },
  "author": "Anthony Scotti",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/amscotti/pwned-passwords-web.git"
  },
  "homepage": "https://128bit.io",
  "dependencies": {
    "angular": "^1.7.0",
    "js-sha1": "^0.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.0",
    "css-loader": "^0.28.11",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-prettier": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.16.1",
    "rimraf": "^2.5.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md,html}": [
      "prettier --write",
      "git add"
    ]
  }
}
